{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "no-console": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "'.*'"
      }
    ],
    "object-shorthand-properties-first": false
  }
}